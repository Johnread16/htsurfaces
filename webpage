      var video = document.querySelectorAll('video');

      $('#carousel-home-captions').on('slide.bs.carousel', function (e) {
        $(video).each(function (ix, obj) {
          if ($(e.relatedTarget).index() == ix) {
            obj.currentTime = 0;
            obj.play();
           
          }
          //else {
          //    obj.pause();
          //    obj.currentTime = 0
          //}
        });
      });
      $('#carousel-home-captions').on('slid.bs.carousel', function (e) {
        setFocus();
      });
      setFocus();
      function setFocus() { /// This function sets focus to the active slide of the carousel for keyboard navigation
        var dots = $('.carousel-indicators li');
        dots.each(function () {
          var item = $(this)
          if (item.hasClass('active')) {
            
            item.attr({ 'aria-selected': 'true', 'tabindex': '0' })
          } else {
            item.attr({ 'aria-selected': 'false', 'tabindex': '-1' })

          }
        });
      }
    });
    function openHgtvModal() {
      $('#myHgtvModal').slideDown('slow');
      $('#myHgtvModal').addClass('in');
      $('<div class="modal-backdrop"></div>').appendTo(document.body);
    }
    function Hgtvmodalclose() {
      $(".modal-backdrop").remove();
      $("#myHgtvModal").slideUp("fast", function () {
      });
      setTimeout(function () {
        $(".dropdown-banner").slideDown("fast", function () {
          // Animation complete.
        })
      }, 10);
    }
