      <script>
 
  var videos = [{ "slide": 1, "src": "expansive_selection-new.mp4" },
  { "slide": 2, "src": "top_5_trends-2023-new.mp4" },
  { "slide": 3, "src": "visualizer.mp4" },

  { "slide": 4, "src": "everlife.mp4" }
  ]
  var owidth = $(window).width();
  $(document).ready(function () {
    setVideoSrc();
    
  });
  
  window.addEventListener("resize", function () {
    var rwidth = $(window).width();
    if (owidth !== rwidth) {
      setVideoSrc();
      owidth = rwidth;
    }
  });
  function setVideoSrc() {
    $(".player").each(function (index, video) {
      
      if (video.firstChild) {
        video.removeChild(video.firstChild);
      }
      $.each(videos, function (i, obj) {
        $('#test-vid').trigger('pause');
        if (index === obj.slide - 1) {
          //  console.log(obj);
          if ($(window).width() < 992) {
            var source = document.createElement('source');
            source.setAttribute('src', 'https://cdn.msisurfaces.com/videos/ipad/' + obj.src);
            source.setAttribute('type', 'video/mp4');
            $(video).prepend(source);
            // video.appendChild(source);
          } else {
            var source = document.createElement('source');
            source.setAttribute('src', 'https://cdn.msisurfaces.com/videos/' + obj.src);
            source.setAttribute('type', 'video/mp4');
            $(video).prepend(source);
            //video.appendChild(source);
          }
          $(video)[0].load();
          $(video)[0].play();
          
        }
        
      });
      
    });
  }
</script>
